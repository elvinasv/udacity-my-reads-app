{
    "extends": ["wesbos"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "react/state-in-constructor": "off",
        "react/destructuring-assignment": "off",
        "no-multi-assign": "off",
        "jsx-a11y/control-has-associated-label": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "tabWidth": 4,
                "singleQuote": true,
                "semi": true
            }
        ]
    }
}
